<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".DetailsScreen">

    <ImageView
        android:id="@+id/imageViewUserScreenHeader_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_245sdp"
        android:background="@drawable/heaaderrectangleloginscreen"
        android:layout_alignParentTop="true"
        android:layout_marginTop="@dimen/_minus20sdp"/>

    <TextView
        android:id="@+id/textViewUserScreenGreet_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_30sdp"
        android:fontFamily="@font/arial"
        android:text="@string/hi_rohan"
        android:textColor="#000"
        android:textSize="@dimen/_27ssp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="@dimen/_35sdp"
        android:layout_marginStart="@dimen/_23sdp"
        android:layout_marginEnd="@dimen/_20sdp"/>

    <TextView
        android:id="@+id/textViewUserScreenText2_1"
        android:layout_width="@dimen/_210sdp"
        android:layout_height="@dimen/_55sdp"
        android:text="Let's start by knowing\nmore about you.."
        android:textColor="#000"
        android:lineSpacingExtra="@dimen/_7sdp"
        android:textSize="@dimen/_20ssp"
        android:fontFamily="@font/arial"
        android:layout_below="@+id/textViewUserScreenGreet_1"
        android:layout_alignParentStart="true"
        android:layout_marginTop="@dimen/_24sdp"
        android:layout_marginStart="@dimen/_23sdp"/>

    <TextView
        android:id="@+id/textViewUserScreenText3_1"
        android:layout_width="@dimen/_242sdp"
        android:layout_height="@dimen/_49sdp"
        android:text="This information helps us to know about your\npersonality type, so that we can provide you\nbetter design and style suggestions"
        android:fontFamily="@font/arial"
        android:textColor="#000"
        android:textSize="@dimen/_12ssp"
        android:lineSpacingExtra="@dimen/_2sdp"
        android:layout_below="@+id/textViewUserScreenText2_1"
        android:layout_marginStart="@dimen/_23sdp"
        android:layout_marginTop="@dimen/_12sdp"/>

    <ProgressBar
        android:id="@+id/progressBarUserScreen_1"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="@dimen/_270sdp"
        android:layout_height="@dimen/_8sdp"
        android:layout_below="@+id/imageViewUserScreenHeader_1"
        android:background="@drawable/progress"
        android:layout_marginTop="@dimen/_34sdp"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginEnd="@dimen/_20sdp"/>

    <ImageButton
        android:id="@+id/imageButtonUserScreenForward_1"
        android:layout_width="@dimen/_26sdp"
        android:layout_height="@dimen/_26sdp"
        android:background="#FFFFFF"
        app:srcCompat="@drawable/ic_arrow_forward_black_24dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="@dimen/_30sdp"
        android:layout_marginEnd="@dimen/_25sdp"/>

    <Switch
        android:id="@+id/switch_1"
        android:layout_width="40dp"
        android:layout_height="24dp"
        android:layout_below="@+id/progressBarUserScreen_1"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="182dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="317dp"
        android:background="@drawable/switchon" />

    <TextView
        android:id="@+id/textViewFemale_1"
        android:layout_width="44dp"
        android:layout_height="15dp"
        android:fontFamily="@font/arial"
        android:textSize="13dp"
        android:textColor="#000"
        android:layout_below="@+id/progressBarUserScreen_1"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="94dp"
        android:layout_marginTop="44dp"
        android:layout_marginBottom="321dp"
        android:text="Female" />

    <TextView
        android:id="@+id/textViewHeight_1"
        android:layout_width="107dp"
        android:layout_height="38dp"
        android:layout_below="@+id/imageViewUserScreenHeader_1"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="67dp"
        android:layout_marginTop="154dp"
        android:layout_marginEnd="237dp"
        android:fontFamily="@font/arial"
        android:text="Height"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="15dp" />

    <EditText
        android:id="@+id/editTextUserScreenHeight_1"
        android:layout_width="262dp"
        android:layout_height="32dp"
        android:layout_below="@+id/imageViewUserScreenHeader_1"

        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="64dp"
        android:layout_marginTop="201dp"
        android:background="@drawable/styleedittextborder"
        android:hint="165"
        android:paddingStart="@dimen/_20sdp"
        android:textColor="#000"
        android:textColorHint="#707070" />

    <TextView
        android:id="@+id/textViewCM_1"
        android:layout_width="30dp"
        android:layout_height="18dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="103dp"

        android:layout_marginBottom="216dp"
        android:text="CM"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textViewWeight_1"
        android:layout_width="111dp"
        android:layout_height="29dp"
        android:layout_below="@+id/imageViewUserScreenHeader_1"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="67dp"
        android:layout_marginTop="233dp"
        android:layout_marginEnd="233dp"
        android:fontFamily="@font/arial"
        android:text="Weight"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="15dp" />

    <EditText
        android:id="@+id/editTextUserScreenBirth_1"
        android:layout_width="262dp"
        android:layout_height="32dp"
        android:layout_below="@+id/imageViewUserScreenHeader_1"

        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="64dp"
        android:layout_marginTop="342dp"
        android:layout_marginBottom="65dp"
        android:background="@drawable/styleedittextborder"
        android:hint="65"
        android:paddingStart="@dimen/_20sdp"
        android:textColor="#000"
        android:textColorHint="#707070" />


    <TextView
        android:id="@+id/textViewBirth_1"
        android:layout_width="156dp"
        android:layout_height="39dp"
        android:layout_below="@+id/imageViewUserScreenHeader_1"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="63dp"
        android:layout_marginTop="300dp"
        android:layout_marginEnd="192dp"
        android:fontFamily="@font/arial"
        android:text="Year of birth"
        android:textColor="#000"
        android:textSize="15dp" />

    <EditText
        android:id="@+id/editTextUserScreenWeight_1"
        android:layout_width="262dp"
        android:layout_height="32dp"
        android:layout_below="@+id/imageViewUserScreenHeader_1"

        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="64dp"
        android:layout_marginTop="263dp"
        android:layout_marginBottom="144dp"
        android:background="@drawable/styleedittextborder"
        android:hint="65"
        android:paddingStart="@dimen/_20sdp"
        android:textColor="#000"
        android:textColorHint="#707070" />

    <TextView
        android:id="@+id/textViewKG_1"
        android:layout_width="30dp"
        android:layout_height="18dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="103dp"
        android:layout_marginBottom="150dp"
        android:text="KG"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textViewMale_2"
        android:layout_width="52dp"
        android:layout_height="29dp"
        android:layout_below="@+id/progressBarUserScreen_1"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="46dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="91dp"
        android:layout_marginBottom="312dp"
        android:layout_toEndOf="@+id/switch_1"
        android:text="Male"
        android:textAlignment="center"
        android:textColor="#000" />


</RelativeLayout>