<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageViewShopScreenGreet_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_130sdp"
        android:layout_alignParentTop="true"
        android:background="@drawable/heaaderrectangleloginscreen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewClosetSugestion_1"
        android:layout_width="@dimen/_117sdp"
        android:layout_height="@dimen/_19sdp"
        android:textSize="18sp"
        android:textStyle="bold"
        android:fontFamily="@font/segoe"
        android:layout_marginTop="@dimen/_17sdp"
        android:text="Closet Sugestion"
        android:textAlignment="center"
        android:layout_marginEnd="@dimen/_160sdp"
        android:layout_marginStart="@dimen/_14sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageViewShopScreenGreet_1" />

    <TextView
        android:id="@+id/textViewAddDesign_1"
        android:layout_width="@dimen/_188sdp"
        android:layout_height="@dimen/_42sdp"
        android:layout_marginEnd="@dimen/_88sdp"
        android:layout_marginTop="@dimen/_52sdp"
        android:layout_marginStart="@dimen/_14sdp"
        android:fontFamily="@font/arial"
        android:text=""
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageViewShopScreenGreet_1"
        app:layout_constraintTop_toTopOf="@+id/imageViewShopScreenGreet_1" />

    <TextView
        android:id="@+id/textViewSeason_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_178sdp"
        android:layout_marginTop="@dimen/_105sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        android:text="Season - Spring/Summer"
        android:textSize="11sp"
        android:textStyle="bold"
        android:fontFamily="@font/arial"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButtonPersonClothesSelection_1"
        android:layout_width="@dimen/_241sdp"
        android:layout_height="@dimen/_301sdp"
        android:layout_marginTop="@dimen/_129sdp"
        android:layout_marginBottom="@dimen/_82sdp"
        android:layout_marginEnd="@dimen/_25sdp"
        android:layout_marginStart="@dimen/_26sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageViewShopFragment1"
        android:layout_width="@dimen/_26sdp"
        android:layout_height="@dimen/_23sdp"
        app:srcCompat="@drawable/cart"
        tools:layout_editor_absoluteX="356dp"
        tools:layout_editor_absoluteY="16dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageViewShopFragment2"
        android:layout_width="@dimen/_24sdp"
        android:layout_height="@dimen/_24sdp"
        app:srcCompat="@drawable/round"
        tools:layout_editor_absoluteX="325dp"
        tools:layout_editor_absoluteY="15dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButtonForward"
        android:layout_width="@dimen/_20sdp"
        android:layout_height="@dimen/_20sdp"
        app:srcCompat="@drawable/backwardarrow"
        android:background="@color/cardview_light_background"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="369dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButtonBackward"
        android:layout_width="@dimen/_20sdp"
        android:layout_height="@dimen/_20sdp"
        app:srcCompat="@drawable/backwardarrow"
        tools:layout_editor_absoluteX="380dp"
        android:background="@color/cardview_light_background"
        tools:layout_editor_absoluteY="369dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButtonDownward"
        android:layout_width="@dimen/_21sdp"
        android:layout_height="@dimen/_21sdp"
        app:srcCompat="@drawable/downarrow"
        android:background="@color/cardview_light_background"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="616dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>